; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="Adrestia"
run/main_scene="res://scenes/title.tscn"
config/icon="res://icon.png"
config/auto_accept_quit=false
config/quit_on_go_back=false

[autoload]

UnitKinds="*res://lib/unit_kinds.gd"
global="*res://global.gd"

[debug]

settings/fps/force_fps=60

[display]

window/size/width=540
window/size/height=960
window/vsync/use_vsync=false
window/handheld/orientation="portrait"
window/handheld/emulate_touchscreen=true
window/stretch/mode="2d"
window/stretch/aspect="keep"
window/stretch/shrink="1"

[rendering]

environment/default_clear_color=Color( 0.492188, 0.492188, 0.492188, 1 )
environment/default_environment="res://default_env.tres"
